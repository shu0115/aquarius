<h1>New rank_vote</h1>

<% form_for(@rank_vote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :election_id %><br />
    <%= f.text_field :election_id %>
  </p>
  <p>
    <%= f.label :selection_id %><br />
    <%= f.text_field :selection_id %>
  </p>
  <p>
    <%= f.label :rank %><br />
    <%= f.text_field :rank %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', rank_votes_path %>