<h1>Listing rank_votes</h1>

<table class="table_style">
  <tr>
    <th>User</th>
    <th>Election</th>
    <th>Selection</th>
    <th>Rank</th>
  </tr>

<% @rank_votes.each do |rank_vote| %>
  <tr>
    <td><%=h rank_vote.user_id %></td>
    <td><%=h rank_vote.election_id %></td>
    <td><%=h rank_vote.selection_id %></td>
    <td><%=h rank_vote.rank %></td>
    <td><%= link_to 'Show', rank_vote %></td>
    <td><%= link_to 'Edit', edit_rank_vote_path(rank_vote) %></td>
    <td><%= link_to 'Destroy', rank_vote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New rank_vote', new_rank_vote_path %>