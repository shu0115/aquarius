<h1>Listing free_choice_votes</h1>

<table class="table_style">
  <tr>
    <th>User</th>
    <th>Election</th>
    <th>Selection</th>
  </tr>

<% @free_choice_votes.each do |free_choice_vote| %>
  <tr>
    <td><%=h free_choice_vote.user_id %></td>
    <td><%=h free_choice_vote.election_id %></td>
    <td><%=h free_choice_vote.selection_id %></td>
    <td><%= link_to 'Show', free_choice_vote %></td>
    <td><%= link_to 'Edit', edit_free_choice_vote_path(free_choice_vote) %></td>
    <td><%= link_to 'Destroy', free_choice_vote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New free_choice_vote', new_free_choice_vote_path %>