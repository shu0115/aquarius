<h1>Editing one_vote</h1>

<% form_for(@one_vote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :election_id %><br />
    <%= f.text_field :election_id %>
  </p>
  <p>
    <%= f.label :selection_id %><br />
    <%= f.text_field :selection_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @one_vote %> |
<%= link_to 'Back', one_votes_path %>